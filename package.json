{
  "name": "mmf-blog-vite-vue3",
  "type": "module",
  "version": "2.0.1",
  "private": true,
  "packageManager": "pnpm@8.6.1",
  "author": "lincenying <lincenying@qq.com>",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "prepare": "sh simple-git-hooks.sh",
    "del": "sh delete-ts.sh",
    "serve": "cross-env DEBUG=vite:transform vite",
    "build": "vite build",
    "start": "vite preview --port 7776",
    "workbox": "workbox generateSW workbox-config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.3.18",
    "@lincy/utils": "^0.5.4",
    "@unhead/vue": "^1.8.11",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.7",
    "element-plus": "^2.6.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "pinia": "^2.1.7",
    "prismjs": "^1.29.0",
    "qs": "^6.12.0",
    "store2": "^2.14.3",
    "vue": "3.4.21",
    "vue-content-loader": "^2.0.1",
    "vue-loading-overlay": "^6.0.4",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@lincy/base-config": "^1.1.0",
    "@lincy/eslint-config": "^4.2.5",
    "@lincy/stylelint-config": "^1.17.4",
    "@lincy/vite-plugin-mock": "^3.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.202",
    "@types/md5": "^2.3.5",
    "@types/prismjs": "^1.26.3",
    "@types/qs": "^6.9.12",
    "@unocss/eslint-plugin": "^0.58.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "lint-staged": "^15.2.2",
    "mockjs": "^1.1.0",
    "polyfill-library": "^4.8.0",
    "postcss": "^8.4.35",
    "sass": "^1.71.1",
    "simple-git-hooks": "^2.10.0",
    "stylelint": "^16.2.1",
    "typescript": "^5.4.2",
    "unocss": "^0.58.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-macros": "^2.7.10",
    "vite": "5.1.5",
    "vite-plugin-mock": "^3.0.1",
    "vite-plugin-pwa": "^0.19.2",
    "vue-tsc": "^2.0.6",
    "workbox-cli": "^7.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "esbuild",
        "postcss",
        "prettier"
      ]
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix"
    ]
  }
}
